{
  "name":"Change Page Color",
  "version":"0.0.1",
  "manifest_version":2,
  "description":"webページの色を変えるアプリ。",
  "browser_action":{
    "default_popup":"src/popup.html"
  },
  "options_page":"src/options.html",
  "content_scripts":[
    {
      "matches":["https://*/*","http://*/*"],
      "js":[
        "vendor/jquery-3.2.1.min.js",
        "src/storage.js",
        "src/change_color.js"]
    }
  ],
  "permissions": [
    "tabs",
    "storage"
  ]
}
